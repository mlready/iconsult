<h1>Listing contracts</h1>

<table>
  <tr>
    <th>Agency</th>
    <th>Client</th>
    <th>Consultant</th>
    <th>Name</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Rate decimal{8,2}</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @contracts.each do |contract| %>
  <tr>
    <td><%= contract.agency_id %></td>
    <td><%= contract.client_id %></td>
    <td><%= contract.consultant_id %></td>
    <td><%= contract.name %></td>
    <td><%= contract.start_date %></td>
    <td><%= contract.end_date %></td>
    <td><%= contract.rate_decimal{8,2} %></td>
    <td><%= link_to 'Show', contract %></td>
    <td><%= link_to 'Edit', edit_contract_path(contract) %></td>
    <td><%= link_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Contract', new_contract_path %>
